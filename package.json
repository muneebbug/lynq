{
  "name": "lynq",
  "version": "0.0.0",
  "private": true,
  "repository": "https://github.com/muneebbug/lynq",
  "homepage": "https://nuxt3-starter-bay.vercel.app",
  "scripts": {
    "build": "prisma generate && nuxt build",
    "dev": "nuxt dev",
    "generate": "prisma generate && nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "release": "npm run lint && npm run typecheck && changelogen --release && git push --follow-tags",
    "lint": "eslint .",
    "typecheck": "nuxi typecheck",
    "prisma:generate": "npx prisma generate",
    "prisma:db:push": "npx prisma db push",
    "prisma:db:pull": "npx prisma db pull",
    "prisma:migrate": "npx prisma migrate dev",
    "prisma:seed": "pnpm prisma:db:push --force-reset && npx prisma db seed",
    "prisma:reset": "npx prisma migrate reset",
    "prisma:studio": "npx prisma studio"
  },
  "prisma": {
    "seed": "ts-node-dev -P tsconfig.server.json prisma/seed.ts"
  },
  "devDependencies": {
    "@nuxt/devtools": "^1.4.1",
    "@nuxt/eslint": "^0.5.6",
    "@nuxt/icon": "^1.5.1",
    "@nuxtjs/color-mode": "^3.4.4",
    "@nuxtjs/tailwindcss": "^6.12.1",
    "@types/ua-parser-js": "^0.7.39",
    "changelogen": "^0.5.5",
    "eslint": "^9.10.0",
    "nuxt": "^3.13.1",
    "nuxt-security": "2.0.0-rc.9",
    "prisma": "^5.19.1",
    "shadcn-nuxt": "^0.10.4",
    "typescript": "^5.6.2"
  },
  "dependencies": {
    "@formkit/auto-animate": "^0.8.2",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "5.19.1",
    "@sidebase/nuxt-auth": "^0.9.2",
    "@trpc/client": "^10.45.2",
    "@trpc/server": "^10.45.2",
    "@vee-validate/zod": "^4.13.2",
    "@vueuse/core": "^11.0.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "lucide-vue-next": "^0.439.0",
    "next-auth": "4.22.5",
    "radix-vue": "^1.9.5",
    "superjson": "^2.2.1",
    "tailwind-merge": "^2.5.2",
    "tailwindcss-animate": "^1.0.7",
    "trpc-nuxt": "^0.10.22",
    "ts-pattern": "^5.3.1",
    "ua-parser-js": "^1.0.38",
    "vee-validate": "^4.13.2",
    "zod": "^3.23.8"
  },
  "overrides": {
    "next-auth": "4.22.5"
  },
  "./core": {
    "types": "./core/index.d.ts",
    "default": "./core/index.js"
  }
}